<%- include('./includes/top.ejs'); %>
</head>
<body>
    <%- include('./includes/nav.ejs'); %>
    <dir class="content">
            <% if (users.length > 0) { %>
                <h3>List of the users:</h3>
                <ul>
                    <% for (let user of users) { %>
                        <li><%= user.name %></li>
                    <% } %>
                </ul>
            <% } else { %>
                <h3>No users registered!</h3>
            <% } %>

    </dir>
</body>
</html>